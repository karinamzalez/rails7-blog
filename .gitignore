# See https://help.github.com/articles/ignoring-files for more about ignoring files.

# Ignore bundler config and downloaded gems
/.bundle
/vendor/bundle

# Ignore all environment files (except templates)
/.env*
!/.env*.erb

# Ignore all logfiles and tempfiles
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore assets
/public/assets
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore sprockets cache
/public/assets
.sprockets-manifest-*.json

# Ignore master key and credentials
/config/master.key
/config/credentials/*.key
/config/credentials.yml.enc

# Ignore database configuration
/config/database.yml

# Ignore test coverage results
/coverage/

# Ignore precompiled javascript packs
/public/packs
/public/packs-test
/node_modules
/yarn-error.log

# Ignore system-specific files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore IDE and editor files
.idea/
.vscode/
*.swp
*.swo
*~
.project
.settings/
.classpath

# Ignore Ruby version manager files
.ruby-gemset
.ruby-version

# Ignore local development files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore database dumps
*.sql
*.sqlite3
*.sqlite3-journal

# Ignore test files
/test/reports/
/spec/reports/
/spec/examples.txt

# Ignore documentation
/doc/
/rdoc/

# Ignore dependency caching files
.npm
.eslintcache
.stylelintcache

# Ignore Webpack build output
/public/webpack/
/app/assets/builds/*
!/app/assets/builds/.keep
